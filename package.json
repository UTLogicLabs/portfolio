{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "A portfolio website built with React, TypeScript, Vite, and Tailwind CSS",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/",
    "icons": "sly add",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:coverage && npm run test:e2e"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.9.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@sly-cli/sly": "^1.12.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "jsdom": "^27.0.0",
    "msw": "^2.3.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "supertest": "^6.3.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vite-plugin-icons-spritesheet": "^1.0.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "vite",
    "tailwind"
  ],
  "author": "Joshua Dix <josh.dix@theblindman.com>",
  "license": "MIT"
}
